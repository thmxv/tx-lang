fn f0() { return 0; }
std_println(f0()); # expect: 0

fn f1(a) { return a; }
std_println(f1(1)); # expect: 1

fn f2(a, b) { return a + b; }
std_println(f2(1, 2)); # expect: 3

fn f3(a, b, c) { return a + b + c; }
std_println(f3(1, 2, 3)); # expect: 6

fn f4(a, b, c, d) { return a + b + c + d; }
std_println(f4(1, 2, 3, 4)); # expect: 10

fn f5(a, b, c, d, e) { return a + b + c + d + e; }
std_println(f5(1, 2, 3, 4, 5)); # expect: 15

fn f6(a, b, c, d, e, f) { return a + b + c + d + e + f; }
std_println(f6(1, 2, 3, 4, 5, 6)); # expect: 21

fn f7(a, b, c, d, e, f, g) { return a + b + c + d + e + f + g; }
std_println(f7(1, 2, 3, 4, 5, 6, 7)); # expect: 28

fn f8(a, b, c, d, e, f, g, h) { return a + b + c + d + e + f + g + h; }
std_println(f8(1, 2, 3, 4, 5, 6, 7, 8)); # expect: 36
